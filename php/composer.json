{
    "name": "sbom-test/php-component",
    "description": "PHP component for SBOM testing",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/http-foundation": "4.4.0",
        "guzzlehttp/guzzle": "6.5.5",
        "monolog/monolog": "2.0.0",
        "doctrine/orm": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SbomTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SbomTest\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "audit": {
            "block-insecure": false
        }
    }
}
