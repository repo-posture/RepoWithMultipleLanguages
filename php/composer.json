{
    "name": "sbom-test/php-component",
    "description": "PHP component for SBOM testing",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.2",
        "laravel/framework": "^10.0",
        "guzzlehttp/guzzle": "^7.5",
        "monolog/monolog": "^3.3",
        "doctrine/orm": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "SbomTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SbomTest\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
